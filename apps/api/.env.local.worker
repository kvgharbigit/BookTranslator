# Environment variables for RQ worker
# Source this before running: source .env.local.worker

# Fix macOS fork() issue with WeasyPrint
export OBJC_DISABLE_INITIALIZE_FORK_SAFETY=YES

# Load main environment
if [ -f .env.local ]; then
    export $(grep -v '^#' .env.local | xargs)
fi

echo "âœ… Worker environment loaded"
